@model LearningHub.Nhs.Auth.Models.Account.LogoutViewModel

@{
    Layout = "_Signout_Layout";
}
<div class="lh-top-info">
    <div class="row">
        <div class="col-sm-6">
            <p>Would you like to logout?</p>
            <form asp-action="Logout">
                <input type="hidden" name="logoutId" value="@Model.LogoutId" />
                <fieldset>
                    <div class="form-group">
                        <button class="btn btn-primary">Yes</button>
                    </div>
                </fieldset>
            </form>
        </div>
    </div>
</div>